<?xml version="1.0" encoding="iso-8859-1"?>

<form>
  <title>Upload</title>
  <name>uploadform</name>
  <action></action>
  <enctype></enctype>
  <method>POST</method>

  <groups>
    <group>
      <title>Default</title>
      <fields>
      
        <field><id>csv_character_set</id> <type>ListField</type>
          <values>
            <alternate_name></alternate_name>
            <css_class></css_class>
            <default></default>
            <description>Character set of the data set.</description>
            <enabled type="int">1</enabled>
            <external_validator></external_validator>
            <extra></extra>
            <extra_item></extra_item>
            <first_item type="int">0</first_item>
            <hidden type="int">0</hidden>
            <items type="list">[('Use specified character set below.','default'), ('Unicode (UTF-8)', 'utf-8'), ('Western Europe (ISO-8859-1)', 'ISO-8859-1'), ('Western Europe (ISO-8859-15/EURO)', 'ISO-8859-15'), ('Western Europe (ASCII/US)', 'ASCII'), ('Western Europe (Windows-1252/WinLatin 1)', 'cp1252'), ('Cyrillic (ISO-8859-5)', 'ISO-8859-5')]</items>
            <required type="int">1</required>
            <size type="int">1</size>
            <title>character set</title>
            <unicode type="int">0</unicode>
            <whitespace_preserve type="int">0</whitespace_preserve>
          </values>
          <tales>
          </tales>
          <messages>
            <message name="external_validator_failed">The input failed the external validator.</message>
            <message name="required_not_found">Input is required but no input given.</message>
            <message name="unknown_selection">You selected an item that was not in the list.</message>
          </messages>
        </field>
        <field><id>csv_dataencoding</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default></default>
          <description>Specify the character set of the data set manually.</description>
          <display_maxwidth type="int">80</display_maxwidth>
          <display_width type="int">40</display_width>
          <external_validator></external_validator>
          <extra></extra>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <required type="int">0</required>
          <title>character set</title>
          <truncate type="int">0</truncate>
          <unicode type="int">0</unicode>
          <whitespace_preserve type="int">0</whitespace_preserve>
        </values>
        <tales>
          <default>python:form.REQUEST.model.data_encoding()</default>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="too_long">Too much input was given.</message>
        </messages>
      </field>
      <field><id>file</id> <type>FileField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default></default>
          <description>File to upload as data set.</description>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">40</display_width>
          <external_validator></external_validator>
          <extra></extra>
          <required type="int">0</required>
          <hidden type="int">0</hidden>
          <title>file</title>
        </values>
        <tales>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
        </messages>
      </field>
      </fields>
    </group>
  </groups>
</form>
