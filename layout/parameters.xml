<?xml version="1.0" encoding="iso-8859-1"?>

<form>
  <title>Parameters form</title>
  <name>form</name>
  <action></action>
  <enctype></enctype>
  <method>POST</method>

  <groups>
    <group>
      <title>Default</title>
      <fields>

        <field><id>table</id> <type>StringField</type>
          <values>
            <alternate_name></alternate_name>
            <css_class></css_class>
            <default></default>
            <description></description>
            <display_maxwidth></display_maxwidth>
            <display_width type="int">20</display_width>
            <external_validator></external_validator>
            <extra></extra>
            <hidden type="int">0</hidden>
            <max_length></max_length>
            <required type="int">1</required>
            <title>table</title>
            <truncate type="int">0</truncate>
            <unicode type="int">0</unicode>
            <whitespace_preserve type="int">0</whitespace_preserve>
          </values>
          <tales>
          </tales>
          <messages>
            <message name="external_validator_failed">The input failed the external validator.</message>
            <message name="required_not_found">Input is required but no input given.</message>
            <message name="too_long">Too much input was given.</message>
          </messages>
        </field>
        <field><id>columns</id> <type>StringField</type>
          <values>
            <alternate_name></alternate_name>
            <css_class></css_class>
            <default></default>
            <description>Comma seperated list of columns to select</description>
            <display_maxwidth></display_maxwidth>
            <display_width type="int">20</display_width>
            <external_validator></external_validator>
            <extra></extra>
            <hidden type="int">0</hidden>
            <max_length></max_length>
            <required type="int">1</required>
            <title>columns</title>
            <truncate type="int">0</truncate>
            <unicode type="int">0</unicode>
            <whitespace_preserve type="int">0</whitespace_preserve>
          </values>
          <tales>
          </tales>
          <messages>
            <message name="external_validator_failed">The input failed the external validator.</message>
            <message name="required_not_found">Input is required but no input given.</message>
            <message name="too_long">Too much input was given.</message>
          </messages>
        </field>
        <field><id>show_caption</id> <type>CheckBoxField</type>
          <values>
            <alternate_name></alternate_name>
            <css_class></css_class>
            <default type="int">1</default>
            <description></description>
            <external_validator></external_validator>
            <extra></extra>
            <hidden type="int">0</hidden>
            <title>show caption</title>
          </values>
          <tales>
          </tales>
          <messages>
            <message name="external_validator_failed">The input failed the external validator.</message>
          </messages>
        </field>
        <field><id>table_class</id> <type>ListField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default></default>
          <description>Choose a style of the table which fits in your needs.</description>
          <enabled type="int">1</enabled>
          <external_validator></external_validator>
          <extra></extra>
          <extra_item></extra_item>
          <first_item type="int">0</first_item>
          <hidden type="int">0</hidden>
          <items type="list">[('Simple table (no formatting)', 'simple'), ('Plain table (bold heading, no borders)', 'plain'), ('List table (bold underlined heading, borderless rows)', 'list'), ('Grid table (bold bordered heading, bordered rows)', 'grid'), ('Data grid table (bold borderless heading, bordered rows)', 'datagrid')]</items>
          <required type="int">0</required>
          <size type="int">1</size>
          <title>table style</title>
          <unicode type="int">0</unicode>
          <whitespace_preserve type="int">0</whitespace_preserve>
        </values>
        <tales>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="unknown_selection">You selected an item that was not in the list.</message>
        </messages>
      </field>
