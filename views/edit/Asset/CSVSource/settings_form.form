<?xml version="1.0" encoding="UTF-8"?>

<form>
  <title>settings</title>
  <name>settings</name>
  <action></action>
  <enctype></enctype>
  <method>POST</method>

  <groups>
    <group>
      <title>Default</title>
      <fields>

      <field><id>csv_headings</id> <type>CheckBoxField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default type="int">0</default>
          <description>Use first row of data as table heading.</description>
          <external_validator></external_validator>
          <extra></extra>
          <hidden type="int">0</hidden>
          <title>use headings</title>
        </values>
        <tales>
          <default>python:form.REQUEST.model.has_headings()</default>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
        </messages>
      </field>
      <field><id>csv_table_class</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default></default>
          <description>Specify a how the table should look like: 'green',
            'red', 'black1', 'black_border'</description>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <enabled type="int">1</enabled>
          <external_validator></external_validator>
          <extra></extra>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <required type="int">0</required>
          <title>css table class</title>
          <truncate type="int">0</truncate>
          <unicode type="int">0</unicode>
          <whitespace_preserve type="int">1</whitespace_preserve>
        </values>
        <tales>
          <default>python:form.REQUEST.model.get_table_class()</default>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="too_long">Too much input was given.</message>
        </messages>
      </field>
      </fields>
    </group>
  </groups>
</form>
