<?xml version="1.0" encoding="UTF-8"?>

<form>
  <title>settings</title>
  <name>settings</name>
  <action></action>
  <enctype></enctype>
  <method>POST</method>

  <groups>
    <group>
      <title>Default</title>
      <fields>

      <field><id>csv_description</id> <type>TextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <external_validator></external_validator>
          <extra></extra>
          <height type="int">5</height>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <max_linelength></max_linelength>
          <max_lines></max_lines>
          <required type="int">0</required>
          <title>Description</title>
          <description>
            Optional description
          </description>
          <unicode type="int">1</unicode>
          <whitespace_preserve type="int">0</whitespace_preserve>
          <width type="int">40</width>
        </values>
        <tales>
          <default>python:form.REQUEST.model.description()</default>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="too_many_lines">You entered too many lines.</message>
          <message name="line_too_long">A line was too long.</message>
          <message name="too_long">You entered too many characters.</message>
        </messages>
      </field>
      <field><id>csv_headings</id> <type>CheckBoxField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default type="int">0</default>
          <description>Use first row of data as table heading.</description>
          <external_validator></external_validator>
          <extra></extra>
          <hidden type="int">0</hidden>
          <title>use headings</title>
        </values>
        <tales>
          <default>python:form.REQUEST.model.has_headings()</default>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
        </messages>
      </field>
      <field><id>csv_dataencoding</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <description>Encoding of the original data (i.e.: ascii, iso-8859-15)</description>
          <display_maxwidth type="int">50</display_maxwidth>
          <display_width type="int">25</display_width>
          <external_validator></external_validator>
          <extra></extra>
          <hidden type="int">0</hidden>
          <max_length type="int">50</max_length>
          <required type="int">1</required>
          <title>encoding</title>
          <truncate type="int">0</truncate>
          <unicode type="int">0</unicode>
          <whitespace_preserve type="int">0</whitespace_preserve>
        </values>
        <tales>
          <default>python:form.REQUEST.model.data_encoding()</default>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="too_long">Too much input was given.</message>
        </messages>
      </field>
      </fields>
    </group>
  </groups>
</form>
