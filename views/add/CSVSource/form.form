<?xml version="1.0" encoding="UTF-8"?>

<form>
  <title></title>
  <name>add_form</name>
  <action></action>
  <enctype>None</enctype>
  <method>POST</method>

  <groups multiple="true">
    <group>
      <title>default</title>
      <fields multiple="true">

      <field><id>object_id</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">1</required>
          <css_class>store</css_class>
          <default></default>
          <title>id</title>
          <truncate type="int">0</truncate>
          <description>No spaces or special characters besides &amp;#8216;_&amp;#8217; or &amp;#8216;-&amp;#8217; or &amp;#8216;.&amp;#8217;</description>
          <extra></extra>
          <unicode>1</unicode>
        </values>
        <tales>
        </tales>
      </field>
      <field><id>object_title</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">1</required>
          <css_class>store</css_class>
          <default></default>
          <title>title</title>
          <description>The title will be publicly visible, and is used for the link in indexes.</description>
          <extra></extra>
          <unicode>1</unicode>
        </values>
        <tales>
        </tales>
      </field>
      <field><id>object_character_set</id> <type>ListField</type>
          <values>
            <alternate_name></alternate_name>
            <css_class>store</css_class>
            <default></default>
            <description>Character coding of the source data.</description>
            <enabled type="int">1</enabled>
            <external_validator></external_validator>
            <extra></extra>
            <extra_item></extra_item>
            <first_item type="int">0</first_item>
            <hidden type="int">0</hidden>
            <items type="list">[('Use specified character coding below.','default'), ('Unicode (UTF-8)', 'utf-8'), ('Western Europe (ISO-8859-1)', 'ISO-8859-1'), ('Western Europe (ISO-8859-15/EURO)', 'ISO-8859-15'), ('Western Europe (ASCII/US)', 'ASCII'), ('Western Europe (Windows-1252/WinLatin 1)', 'cp1252'), ('Western Europe (Apple Macintosh)', 'mac_roman'), ('Cyrillic (ISO-8859-5)', 'ISO-8859-5')]</items>
            <required type="int">1</required>
            <size type="int">1</size>
            <title>character coding</title>
            <unicode type="int">0</unicode>
            <whitespace_preserve type="int">0</whitespace_preserve>
          </values>
          <tales>
          </tales>
          <messages>
            <message name="external_validator_failed">The input failed the external validator.</message>
            <message name="required_not_found">Input is required but no input given.</message>
            <message name="unknown_selection">You selected an item that was not in the list.</message>
          </messages>
        </field>
        <field><id>object_dataencoding</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class>store</css_class>
          <default>latin1</default>
          <description>Specify the character coding of the source data manually.</description>
          <display_maxwidth type="int">80</display_maxwidth>
          <display_width type="int">40</display_width>
          <external_validator></external_validator>
          <extra></extra>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <required type="int">0</required>
          <title>designation of coding</title>
          <truncate type="int">0</truncate>
          <unicode type="int">0</unicode>
          <whitespace_preserve type="int">0</whitespace_preserve>
        </values>
        <tales>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="too_long">Too much input was given.</message>
        </messages>
      </field>
      <field><id>object_file</id> <type>FileField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default></default>
          <description>File to upload as source data.</description>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">40</display_width>
          <external_validator></external_validator>
          <extra></extra>
          <required type="int">0</required>
          <hidden type="int">0</hidden>
          <title>csv file</title>
        </values>
        <tales>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
        </messages>
      </field>
      </fields>
    </group>
  </groups>
</form>
