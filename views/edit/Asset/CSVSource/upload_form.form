<?xml version="1.0" encoding="iso-8859-1"?>

<form>
  <title>Upload</title>
  <name>uploadform</name>
  <action></action>
  <enctype></enctype>
  <method>POST</method>

  <groups>
    <group>
      <title>Default</title>
      <fields>

      <field><id>csv_dataencoding</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default></default>
          <description>Encoding of the data set.</description>
          <display_maxwidth type="int">80</display_maxwidth>
          <display_width type="int">40</display_width>
          <external_validator></external_validator>
          <extra></extra>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <required type="int">1</required>
          <title>encoding</title>
          <truncate type="int">0</truncate>
          <unicode type="int">0</unicode>
          <whitespace_preserve type="int">0</whitespace_preserve>
        </values>
        <tales>
          <default>python:form.REQUEST.model.data_encoding()</default>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="too_long">Too much input was given.</message>
        </messages>
      </field>
      <field><id>file</id> <type>FileField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default></default>
          <description>File to upload as data set.</description>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">40</display_width>
          <external_validator></external_validator>
          <extra></extra>
          <required type="int">0</required>
          <hidden type="int">0</hidden>
          <title>File</title>
        </values>
        <tales>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
        </messages>
      </field>
      </fields>
    </group>
  </groups>
</form>
